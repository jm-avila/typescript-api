{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Docker: Attach to Node",
        "port": 9229,
        "skipFiles": [
          "${workspaceFolder}/node_modules/**/*.js",
          "<node_internals>/**/*"
        ],
        "address": "localhost",
        "sourceMapPathOverrides": {
          "/app/*": "${workspaceRoot}/*"
        },
        "timeout": 35000,
        "restart": true
      }
    ]
  }